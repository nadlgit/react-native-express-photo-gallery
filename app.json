{
  "expo": {
    "name": "photo-gallery",
    "slug": "react-native-express-photo-gallery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/app/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/app/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nltest.photogallery"
    },
    "web": {
      "favicon": "./src/app/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bba0476b-51bd-4554-9f31-e6fc15c3df1f"
      }
    }
  }
}
